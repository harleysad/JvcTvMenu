<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="@color/startcolor" android:endColor="@color/endcolor" android:angle="270" android:centerY="0.5" android:centerColor="@color/centercolor"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@drawable/barcolor" android:endColor="@drawable/barcolor" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
